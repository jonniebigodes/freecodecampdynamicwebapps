    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.4.0",
    "css-loader": "0.23.1",
    "dotenv": "^4.0.0",
