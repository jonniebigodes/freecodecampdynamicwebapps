    "node-sass": "^3.4.2",
    "react": "^15.0.1",
    "react-bootstrap": "^0.30.7",
    "react-chartjs-2": "^2.0.0",
    "react-date-picker": "^5.3.28",
    "react-dom": "^15.0.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "sass-loader": "^3.2.0",
    "socket.io": "^1.7.2",
    "socket.io-react": "^1.1.2",
    "style-loader": "^0.13.1",
    "unirest": "^0.5.1",
    "url-loader": "^0.5.7",
