{
  "name": "freecodecampdynamicwebapps",
  "version": "1.0.0",
  "description": "freecodecamp dynamic challenges webapp",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "watch": "webpack -d --watch",
    "postinstall": "webpack -p --config webpack.prod.config.js",
    "showerrors": "webpack --progress --color --watch --display-error-details",
    "showinfo": "webpack --display-modules --display-reasons",
    "showinfofile": "webpack --json>stats.json",
    "showfilesizefile": "webpack --display-modules --display-reasons --sort-modules-by size --json>stats.json",
    "spiterror": "webpack --display-error-details --json>errors.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonniebigodes/freecodecampdynamicwebapps.git"
  },
  "keywords": [
    "heroku",
    "dynamic apps",
    "freecodecamp",
    "challenges"
  ],
  "author": "jonniebigodes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonniebigodes/freecodecampdynamicwebapps/issues"
  },
  "homepage": "https://github.com/jonniebigodes/freecodecampdynamicwebapps#readme",
