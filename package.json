    "async": "^2.1.4",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-minify": "^0.2.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.4.0",
    "css-loader": "0.23.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-session": "^1.15.3",
    "file-loader": "^0.10.0",
    "material-design-icons": "^3.0.1",
    "material-ui": "^0.18.7",
    "mongodb": "^2.2.24",
    "node-sass": "^3.4.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "prop-types": "^15.5.10",
    "react": "^15.0.1",
    "react-bootstrap": "^0.30.7",
    "react-chartjs-2": "^2.0.0",
    "react-date-picker": "^5.3.28",
    "react-dom": "^15.0.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
